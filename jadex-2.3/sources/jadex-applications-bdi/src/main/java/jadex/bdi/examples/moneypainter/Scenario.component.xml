<componenttype xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex
						http://jadex.sourceforge.net/jadex-component-2.3.xsd"
	name="Scenario" package="jadex.bdi.examples.moneypainter">
	
	<componenttypes>
		<componenttype name="rich" filename="jadex/bdi/examples/moneypainter/Rich.agent.xml"/>
		<componenttype name="painter" filename="jadex/bdi/examples/moneypainter/Painter.agent.xml"/>
	</componenttypes>
	
	<configurations>
		<configuration name="One Rich, one Painter">
			<components>
				<component type="rich"/>
				<component type="painter"/>
			</components>
		</configuration>
		
		<configuration name="One Rich, three Painters">
			<components>
				<component type="rich"/>
				<component type="painter" number="3"/>
			</components>
		</configuration>
	</configurations>
</componenttype>
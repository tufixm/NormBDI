<?xml version="1.0" encoding="UTF-8"?>
<!--
	<H3>Adder component.</H3>
	
	Very simple component that provides an add service.
-->
<componenttype xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex
						http://jadex.sourceforge.net/jadex-component-2.3.xsd"
	name="Adder" package="jadex.micro.testcases.semiautomatic.compositeservice">
	
	<arguments>
		<argument name="arg" class="String">"default"</argument>
	</arguments>
	
	<services>
		<providedservice class="IAddService">
			<implementation>new AddService($component)</implementation>
		</providedservice>
	</services>
</componenttype>